{
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"node_modules"
	],
	"compilerOptions": {
		"target": "esNext",
		"module": "esNext",
		"moduleResolution": "Node",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"strictNullChecks": true,
		"noImplicitAny": true,
		"outDir": "./dist",
		"rootDir": "src",
		"allowJs": true,
		"checkJs": true
	}
}
